{
  "name": "@konnec/vue-eloquent",
  "version": "1.2.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage.enabled=true --coverage.reporter=html",
    "test:unit:ui": "vitest --ui --coverage.enabled=true --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.d.ts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "axios": "^1.6.8",
    "laravel-echo": "^1.16.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.1",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-istanbul": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "@vue/devtools-api": "^6.6.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "^24.0.0",
    "msw": "^2.2.13",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.1.7",
    "prettier": "^3.2.5",
    "pusher-js": "^8.3.0",
    "pusher-js-mock": "^0.3.8",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.7"
  },
  "description": "An interface for Vue and Laravel Eloquent through API",
  "main": "./src/index.ts",
  "type": "module",
  "module": "./src/index.ts",
  "types": "./src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-eloquent.git"
  },
  "keywords": [
    "Vue",
    "Laravel",
    "Api",
    "Eloquent",
    "ORM",
    "Model",
    "Validation",
    "Broadcast",
    "Vuelidate",
    "Echo",
    "DevTools"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-eloquent/issues"
  },
  "homepage": "https://vue-eloquent.netlify.app"
}
