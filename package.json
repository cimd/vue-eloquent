{
  "name": "@konnec/vue-eloquent",
  "version": "0.4.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "axios": "^1.4.0",
    "laravel-echo": "^1.15.1",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vitest/ui": "^0.30.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.6.1"
  },
  "description": "An interface for Vue and Laravel Eloquent through API",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-eloquent.git"
  },
  "keywords": [
    "Vue",
    "Laravel",
    "Api",
    "Eloquent",
    "Model",
    "Validation",
    "Broadcast",
    "Vuelidate",
    "Echo"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-eloquent/issues"
  },
  "homepage": "https://github.com/cimd/vue-eloquent#readme"
}
