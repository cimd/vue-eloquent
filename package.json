{
  "name": "@konnec/vue-eloquent",
  "version": "0.7.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "axios": "^1.4.0",
    "laravel-echo": "^1.15.2",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.1",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/jsdom": "^21.1.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitest/ui": "^0.34.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "pusher-js": "^8.3.0",
    "typescript": "4.9.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "vue-tsc": "^1.8.8"
  },
  "description": "An interface for Vue and Laravel Eloquent through API",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/cimd/vue-eloquent.git"
  },
  "keywords": [
    "Vue",
    "Laravel",
    "Api",
    "Eloquent",
    "Model",
    "Validation",
    "Broadcast",
    "Vuelidate",
    "Echo"
  ],
  "author": "Christian D'Aquino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cimd/vue-eloquent/issues"
  },
  "homepage": "https://github.com/cimd/vue-eloquent#readme"
}
